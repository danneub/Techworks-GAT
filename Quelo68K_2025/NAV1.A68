	 SUB.B	   #$80,D5	       +08	 D5-$80    ($80=MUTE)
	 NEG.B	   D5		       +08	 $80-D5 = WHITE NOISE LEVEL
	 MOVE.B    D5,ONV1WNL(A6)      +08	 OUTPUT WHITE NOISE LEVEL
