	 BTST.B    #IMONADF1,IMPSDI2(A6) JUMP IF INPUT BIT IS NOT SET
	 BEQ.S	   SET3
	 BCLR.B    #OSSADF1N,NASCONT1  CLR STUDENT SELECT ADF1 (NOT)
SET3	 EQU	   *
