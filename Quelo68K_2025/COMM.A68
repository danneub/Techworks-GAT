	 BSET.B    #OSSNAV2N,NASCONT1  SET STUDENT SELECT NAV2 (NOT)
	 BTST.B    #IMONNAV2,IMPSDI2(A6) JUMP IF INPUT BIT IS NOT SET
	 BEQ.S	   SET2
	 BCLR.B    #OSSNAV2N,NASCONT1  CLR STUDENT SELECT NAV2 (NOT)
SET2	 EQU	   *
*
*    OSSADF1N = IMONADF1 (NOT)
