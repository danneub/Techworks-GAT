SET0	 EQU	   *
*
*    OSSNAV1N = IMONNAV1 (NOT)
*
	 BSET.B    #OSSNAV1N,NASCONT1  SET STUDENT SELECT NAV1 (NOT)
	 BTST.B    #IMONNAV1,IMPSDI2(A6) JUMP IF INPUT BIT IS NOT SET
	 BEQ.S	   SET1
	 BCLR.B    #OSSNAV1N,NASCONT1  CLR STUDENT SELECT NAV1 (NOT)
SET1	 EQU	   *
