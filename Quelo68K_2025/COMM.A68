	 BSET.B    #OSSADF2N,NASCONT1  STUDENT SELECT ADF2 (NOT)
	 BSET.B    #OSSDME1N,NASCONT2  STUDENT SELECT DME1 (NOT)
	 BSET.B    #OSSDME2N,NASCONT2  STUDENT SELECT DME2 (NOT)
